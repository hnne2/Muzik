<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/upd"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:foregroundTint="#9A3737"
    tools:context=".mytraks">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:foregroundTint="#C83636"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <TextView
            android:id="@+id/mRecordbtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView"
            style="@android:style/Widget.TextView.PopupMenu"
            android:layout_width="164dp"
            android:layout_height="45dp"
            android:layout_marginTop="70dp"
            android:text="My Library"
            android:textAllCaps="true"
            android:textColor="#E46565"
            android:textSize="20dp"
            android:textStyle="normal|bold|italic"
            app:layout_constraintBottom_toTopOf="@+id/listviesms"
            app:layout_constraintEnd_toStartOf="@+id/addFriend"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/updait_batton"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.058"
            tools:ignore="MissingConstraints" />

        <ListView
            android:id="@+id/listviesms"
            android:layout_width="406dp"
            android:layout_height="434dp"
            android:layout_marginTop="132dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="MissingConstraints" />

        <ImageButton
            android:id="@+id/button_play"
            android:layout_width="73dp"
            android:layout_height="77dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="8dp"
            android:backgroundTint="#00FFFFFF"
            android:contentDescription="TODO"
            android:scaleType="fitCenter"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@android:drawable/ic_menu_add" />

        <ImageButton
            android:id="@+id/button_fire"
            android:layout_width="56dp"
            android:layout_height="63dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:backgroundTint="#00FFFFFF"
            android:contentDescription="Enter How Much Cookies You Want"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="?attr/actionModeCloseDrawable" />

        <ImageButton
            android:id="@+id/imageButton_next"
            android:layout_width="80dp"
            android:layout_height="81dp"
            android:layout_marginEnd="52dp"
            android:layout_marginBottom="24dp"
            android:backgroundTint="#00FFFFFF"
            android:contentDescription="Enter How Much Cookies You Want"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:srcCompat="@android:drawable/ic_media_ff" />

        <ImageButton
            android:id="@+id/Previos_button"
            android:layout_width="82dp"
            android:layout_height="80dp"
            android:layout_marginStart="28dp"
            android:layout_marginBottom="28dp"
            android:backgroundTint="#00FFFFFF"
            android:contentDescription="Enter How Much Cookies You Want"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@android:drawable/ic_media_rew" />

        <ProgressBar
            android:id="@+id/progressBar"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="301dp"
            android:layout_height="30dp"
            android:layout_marginBottom="8dp"
            android:progressTint="#8C0D0D"
            app:layout_constraintBottom_toTopOf="@+id/Play_Batton"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <ImageButton
            android:id="@+id/Play_Batton"
            android:layout_width="94dp"
            android:layout_height="79dp"
            android:layout_marginBottom="28dp"
            android:backgroundTint="#00FFFFFF"
            android:contentDescription="Enter How Much Cookies You Want"
            android:scaleType="fitCenter"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageButton_next"
            app:layout_constraintHorizontal_bias="0.466"
            app:layout_constraintStart_toEndOf="@+id/Previos_button"
            app:srcCompat="@android:drawable/ic_media_next" />

        <ImageButton
            android:id="@+id/addFriend"
            android:layout_width="56dp"
            android:layout_height="63dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="8dp"
            android:backgroundTint="#00FFFFFF"
            android:contentDescription="Enter How Much Cookies You Want"
            android:scaleType="fitCenter"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button_play"
            app:srcCompat="?attr/actionModeShareDrawable" />

        <TextView
            android:id="@+id/final_text"
            android:layout_width="130dp"
            android:layout_height="36dp"
            android:layout_marginStart="76dp"
            android:layout_marginTop="16dp"
            android:text=":"
            android:visibility="gone"
            app:layout_constraintStart_toEndOf="@+id/button_fire"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/updait_batton"
            android:layout_width="56dp"
            android:layout_height="63dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:backgroundTint="#00FFFFFF"
            android:contentDescription="Enter How Much Cookies You Want"
            android:scaleType="fitCenter"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button_fire"
            app:srcCompat="@android:drawable/ic_menu_rotate" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>